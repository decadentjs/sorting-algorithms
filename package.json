{
  "name": "react-starter-hmr",
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "es2015",
            "react"
          ]
        }
      ]
    ]
  },
  "scripts": {
    "build": "NODE_ENV=production mkdirp dist & browserify main.js | uglifyjs -cm > dist/bundle.js",
    "start": "npm run www & npm run watch",
    "watch": "mkdirp dist & watchify main.js -p browserify-hmr -o dist/bundle.js -dv",
    "www": "ecstatic -p 8000 ."
  },
  "dependencies": {
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-react": "^6.1.18",
    "babelify": "^7.2.0",
    "browserify": "^12.0.1",
    "browserify-hmr": "~0.3.1",
    "ecstatic": "~0.8.0",
    "mkdirp": "^0.5.1",
    "react": "~0.14.3",
    "react-dom": "^0.14.3",
    "ud": "^2.0.2",
    "uglifyjs": "^2.4.10",
    "watchify": "^3.6.1"
  },
  "license": "public domain"
}
